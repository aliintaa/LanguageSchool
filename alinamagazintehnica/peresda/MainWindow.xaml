<Window x:Class="peresda.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:peresda"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="131*"/>
            <RowDefinition Height="14*"/>
        </Grid.RowDefinitions>
        <Button Grid.Row="1" x:Name="back" Width="150" HorizontalAlignment="Right" Click="back_Click">назад</Button>
        <Frame x:Name="fram" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
